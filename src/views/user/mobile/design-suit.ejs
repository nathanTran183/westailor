                        for(let i = 0 ; i <= 1; i++) { %>
                            <div class="slide-wrapper front index_0 <%= i == 0 ? 'visible' : ''%>">
                                <div class="slide layers viewport active <%= i == 0 ? 'front' : 'back' %>">

                            <% Object.keys(currentStyle).forEach(key => {
                                let component = product.components.find(component => component.code == key);
                                let componentImg;
                                if (!!component.parentComponent) {
                                    let parentStyle = currentStyle[component.parentComponent];
                                    componentImg = componentImgs.component_item.find(img => {
                                        return img.item_id == currentStyle[key] && img.parent_id == parentStyle
                                    });
                                } else {
                                    componentImg = componentImgs.component_item.find(img => img.item_id == currentStyle[key]);
                                }
                                if(!!componentImg.img_url_front && i == 0) { %>
                                    <img src="<%= componentImg.img_url_front %>" parent="" id="<%=key %>" class="front">
                                    <%  } 
                                if(!!componentImg.img_url_back && i == 1) { %>
                                    <img src="<%= componentImg.img_url_back %>" parent="" id="<%=key %>" class="back">
                                    <%  } 
                            }) %>
                            </div>
                        </div>
                        <% } %>
                    
                </div>
            </div>
        </div>
        <% }) %>
