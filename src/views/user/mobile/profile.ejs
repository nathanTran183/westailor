                        <script type="text/javascript">
                            var confirm_text = "Want to delete your account?";

                            ready_callbacks.push(function () {
                                $('form a#formTrigger').click(function () {
                                    var form = $(this).closest('form');
                                    form.find('#email').prop('disabled', true);
                                    form.find('#fake_password').prop('disabled', true);
                                    form.submit();
                                    return false;
                                });

                                floatingLabel.init();
                                floatingLabel.evaluateInputs();

                                $('#customer-data-form').validate();

                                $('#change-password').click(function () {
                                    $('#change-password-popup').show();
                                });

                                $('#change-password-popup .background,#change-password-popup .close').click(function () {
                                    $('#change-password-popup').hide();
                                })

                                $('#close-password-popup').click(function () {
                                    $('#change-password-popup').hide();
                                });

                                $('#change-password-form').validate({
                                    'rules': {
                                        password: { minlength: 6 },
                                        confirm_password: { equalTo: "#password" }
                                    }
                                });

                                $('.eliminar-cuenta').click(function () {
                                    $('.eliminar-cuenta + .extraInfo').show();
                                });

                                $('.delete_account').click(function (e) {
                                    e.preventDefault();
                                    if (!confirm(confirm_text)) return false;
                                    window.location.href = this.href;
                                });
                            });
                        </script>
