<script type="text/javascript">
	//Common vars:
	var bLazy_fabric = false;

	function menu_types(){

            $(window).resize(function(){
                var width = $(document).width();
                if (width <= 1024){
                    $('.menu_info').addClass('mobile_version');
                    $('#info_common_box .content_info').addClass('full_width');
                }else{
                    $('.menu_info').removeClass('mobile_version');
                    $('#info_common_box .content_info').removeClass('full_width');
                }
            });


			$(window).scroll(function(){
                var width = $(document).width();
                if (width > 1008){
					var scrollTop = $(document).scrollTop();

                    var scrollPosition = window.pageYOffset;
                    var windowSize     = $(document).height();
                    var bodyHeight     = $('.menu_info').height();
					var distanceBottom = windowSize-scrollPosition-bodyHeight;

					var height_content = $('#info_common_box').height();
					if(distanceBottom < 800){
                        $('.menu_info').addClass('fixed_bottom');
                        $('.menu_info').removeClass('fixed');
                    }else if(scrollTop > 120 && height_content > 800){
						$('.menu_info').addClass('fixed');
                        $('.menu_info').removeClass('fixed_bottom');
					}else{
						$('.menu_info').removeClass('fixed');
                        $('.menu_info').removeClass('fixed_bottom');
