	//Funciones comunes para secciones de FAQ/WHY/HOW/MADE/CONTACT/PERFECT
	function enable_blazy() {
		//BLAZY para el Step de Fabric
		if(typeof(Blazy)!='undefined') {

			bLazy_fabric = new Blazy({
				container: '.fabric_container_lazy',
				success: function(element){
					element.parentNode.className = element.parentNode.className.replace(/\loading\b/,'');
				}
			});
		} else {
			bLazy_fabric = false;
		}
		if (bLazy_fabric) bLazy_fabric.revalidate();
	}

	//Mostrar el listado completo de los tejidos:
	function show_all_fabric_list(){
		//Btn: show all fabrics list
		$('.fabric_box.show_all_fabrics').click(function(){
			//mostrar all fabrics
			$(this).closest('.box').find('.hide_elem').css('display','inline-block');

			//eliminar capa de (+XX FABRICS)
			$(this).closest('.box').find('.totals').remove();

			//Revalidar blazy
			if (bLazy_fabric) bLazy_fabric.revalidate();
			return false;
		});
	}

	//Show common fabric preview using magnificPopup:
	function show_fabric_popup(){
		$('.fabric_box:not(".show_all_fabrics") a.fabric, .fabric_box .thumb_preview').magnificPopup({
			type: 'iframe',
			iframe: {
				markup: '<div class="iframe-popup-our-fabrics">'+
