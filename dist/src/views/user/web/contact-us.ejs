				//assign tpl in div
				$('#fabric_preview_layer .info_box').html(data);
				$('#fabric_preview_layer').show();
				return false;
			});
			*/
			return false;
		});

		$('.navigate .back').click(function(){
			$('#fabric_preview_layer').hide();
		});

	}

	//Responsive fabrics
	function responsive_fabrics(){
		$( window ).resize(function() {
			var w = $( window ).width();

			$('.fabric_list').each(function(){

				//Total de cajas por bloke
				var num_fabrics = $(this).find('.fabric_box').length;

				//Anchos x dominio:
				
					var max_w = 1390;
					var middle_1 = 1248;
				

				//Tratamiento
				if(w > max_w){	// t4l: 1390   -   Sumi: 1398
					if(num_fabrics == 7){ //CONTROL: 6 FABRICS + More fabrics
						$(this).find('.last_fabric').hide();
					}
				}else if(w <= 682){
					//Mostrar todos siempre ya que trabajamos en una fila.
					$(this).find('.last_fabric').show();
				}else if(w <= 844){
					if(num_fabrics == 5){ //CONTROL: 5 FABRICS
						$(this).find('.last_fabric').hide();
					}
				}else if(w <= 1087){
					if(num_fabrics == 7){ //CONTROL: 6 FABRICS + More fabrics
						$(this).find('.last_fabric').hide();
					}
					if(num_fabrics == 5){ //CONTROL: 5 FABRICS
						$(this).find('.last_fabric').show();
					}
				}else if(w <= middle_1){ // t4l: 1248   -   Sumi: 1254
					if(num_fabrics == 5){ //CONTROL: 5 FABRICS
						$(this).find('.last_fabric').hide();
					}
					if(num_fabrics == 6){ //CONTROL: 6 FABRICS
						$(this).find('.last_fabric').show();
					}
				}else if(w <= max_w){ // t4l: 1390   -   Sumi: 1398
					if(num_fabrics == 7){ //CONTROL: 6 FABRICS + More fabrics
						$(this).find('.last_fabric').show();
					}
					if(num_fabrics == 6){ //CONTROL: 6 FABRICS
						$(this).find('.last_fabric').hide();
					}
				}

			});

			//window.enable_blazy();

		});
	}



	ready_callbacks.push(function() {
		if (window.mobile_enabled) {
			//Al hacer click en menu_header, desplegar menu_info con accordeon
			$('.menu_header').click(function(){
				$('.menu_info').slideToggle();
