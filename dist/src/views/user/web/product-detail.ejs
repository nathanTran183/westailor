<div>
    
        <% 
    products.forEach((product, index) => {     
    let flag = index == 0 ? "in active" : "";
    let productDesign = cartItem.products.find(prod => prod.product_id == product.code);
    %>
        <div id="<%=product.code%>" class="tab-pane fade <%= flag%>">
            <div id="product_preview">
                <div id="product_preview_image">
                    <div id="model_3d_preview" class="man_suit2">
                        <img src="<%= productDesign.img%>">
                    </div>
                </div>
                <div id="product_preview_details">
                    <div class="title">
                        <p class="subtittle_extras" style="text-transform: uppercase">
                            <%= product.name%>
                        </p>
                    </div>
                    <div class="config">
                        <h2>Details:</h2>
                        <ul>
                            <% 
                    let currentStyle = productDesign.style;
                    Object.keys(currentStyle).forEach(key => {
                        let component = product.components.find(component => component.code == key);
                        let label = component.name;
                        let value = component.componentItems.find(item => item.code == currentStyle[key]);                        
                        %>
                            <li>
                                <p class="label" style="text-transform: capitalize"><%= label %>: </p>
                                <p class="value"><%= value.name %></p>
                            </li>
                            <% }); %>
                        </ul>
                        <h2>Measure:</h2>
                        <% if(!!productDesign.measure) { %>
                        <ul>
                            <%  Object.keys(productDesign.measure).forEach(key => { %>
                            <li>
                                <p class="label" style="text-transform: capitalize"><%= key %>: </p>
                                <p class="value"><%= productDesign.measure[key] %> (cm)</p>
                            </li>
                            <% }); %>
                        </ul>
                        <% } %>
                    </div>
                    <div class="fabric">
                        <h2>Fabric:</h2>
                        <div class="fabric_box ">
                            <div class="fabric_description">
                                <h3 class="marge_inf"><%= fabric.name %></h3>
                                <div class="info">
                                    <div class="marge_inf">
                                        <span class="label">Material: </span>
                                        <span class="value"><%= fabric.material.join(", ") %></span>
                                    </div>
                                    <div class="marge_inf">
                                        <span class="label">Season: </span>
                                        <span class="value"><%= fabric.season.join(", ") %></span>
                                    </div>
                                    <div class="marge_inf">
                                        <span class="label">Color: </span>
                                        <span class="value"><%= fabric.color.join(", ") %></span>
                                    </div>
                                </div>
                            </div>
                            <div class="cloth_preview marge_inf">
                                <img src="<%= fabric.catalog_img %>" title="Oberon">
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
        <% }) %>
    </div>


